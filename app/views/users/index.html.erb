<img src="book.png" alt="book" class="books">


<h1>契約者一覧</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.created_at.strftime('%Y/%m/%d') %></td>
      </tr>
    <% end %>
    <div class="page">
      <%= paginate @users %>
    </div>
  </tbody>
</table>
